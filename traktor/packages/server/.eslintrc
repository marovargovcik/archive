{
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "sort-destructure-keys"
  ],
  "rules": {
    "prettier/prettier": "error",
    "sort-destructure-keys/sort-destructure-keys": "error",
    "sort-keys": [
      "error",
      "asc",
      {
        "caseSensitive": false
      }
    ]
  }
}